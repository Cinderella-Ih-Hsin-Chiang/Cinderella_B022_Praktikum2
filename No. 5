#include <iostream>

using namespace std;

int main()
{
   int matA [2][2]={4,8,2,10};
   
   cout<<"Matriks A"<<endl;
   for (int a=0;a<2;a++){
       for(int b=0;b<2;b++){
        cout<<matA[a][b]<<" ";
       }
       cout<<endl;
   }
   cout<<endl;
  cout<<"Transpose Matriks A \n";
  for (int a=0;a<2;a++){
       for (int b=0;b<2;b++){
    
           cout<<matA[b][a]<<" ";
       }
       cout<<endl;
   }
   
    return 0;
}
