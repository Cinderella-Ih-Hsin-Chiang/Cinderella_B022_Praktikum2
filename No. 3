#include <iostream>
#include <cmath>
using namespace std;

int main(){
   int matA [2][2]={4, 8, 2, 10};
   
   cout<<"Matriks A"<<endl;
   for (int a=0;a<2;a++){
       for(int b=0;b<2;b++){
        cout<<matA[a][b]<<" ";
       }
       cout<<endl;
   }
   cout<<"Hasil Perkalian Eksponen 5 Matrik A "<<endl;
   for (int p=0; p<2; p++){
       for (int q=0; q<2; q++){
           cout<< pow(matA[p][q] * matA[p][q], 5) <<" ";
       }
       cout<<endl;
   }
    return 0;
}
